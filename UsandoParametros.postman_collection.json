							"pm.globals.set(\"segmentProducts0\", \"Test\");",
							"pm.globals.set(\"segmentProducts1\", \"Test\");",
							"pm.globals.set(\"secondPaxLastName\", makeid());",
							"pm.globals.set(\"secondPaxName\", makeid());",
							"pm.globals.set(\"thridPaxLastName\", makeid());",
							"pm.globals.set(\"thirdPaxName\", makeid());",
							"",
							"function makeDate(days) {",
							"    var fechaAUsar = new Date();",
							"    var dd = fechaAUsar.getDate() + days;",
							"    var mm = fechaAUsar.getMonth() + 1;",
							"    var yyyy = fechaAUsar.getFullYear();",
							"",
							"    if (mm < 10) {",
							"        if (dd < 10) {",
							"            fechaAUsar = yyyy + \"-0\" + String(mm) + \"-0\" + String(dd);",
							"        } else {",
							"            fechaAUsar = yyyy + \"-0\" + String(mm) + \"-\" + String(dd);",
							"        }",
							"    } else {",
							"        if (dd < 10) {",
							"            fechaAUsar = yyyy + \"-\" + String(mm) + \"-0\" + String(dd);",
							"        } else {",
							"            fechaAUsar = yyyy + \"-\" + String(mm) + \"-\" + String(dd);",
							"        }",
							"    }",
							"    return fechaAUsar;",
							"}",
							"",
							"function makeid() {",
							"    var text = \"\";",
							"    var possible = \"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz\";",
							"",
							"    for (var i = 0; i < 5; i++)",
							"        text += possible.charAt(Math.floor(Math.random() * possible.length));",
							"",
							"    return text;",
							"}",
							"",
							"if (pm.globals.get(\"checkIn\")===\"true\") {",
							"    pm.globals.set(\"departureDate\", makeDate(0));",
							"    pm.globals.set(\"returnDate\", makeDate(1));",
							"} else {",
							"    pm.globals.set(\"departureDate\", makeDate(9));",
							"    pm.globals.set(\"returnDate\", makeDate(10));",
							"}",
